# Rules to preprocess splicing factor bulk datasets
module sf_perturbations_bulk: 
    snakefile: "sf_perturbations_bulk.smk"
use rule * from sf_perturbations_bulk as bulk_*

# Rules to preprocess splicing factor bulk datasets
module perturbations_singlecell: 
    snakefile: "perturbations_singlecell.smk"
use rule * from perturbations_singlecell as singlecell_*

rule all:
    input:
        rules.bulk_all.input
        rules.singlecell_all.input
    default_target: True